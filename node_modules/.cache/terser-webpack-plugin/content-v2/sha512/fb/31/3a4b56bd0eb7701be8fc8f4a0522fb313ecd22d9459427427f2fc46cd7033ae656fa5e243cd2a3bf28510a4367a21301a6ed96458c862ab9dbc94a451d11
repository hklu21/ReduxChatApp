\"import { createSlice } from '@reduxjs/toolkit'\\nimport { createAsyncThunk } from '@reduxjs/toolkit'\\n\\nconst initialState = [\\n  { id: '', pwd: '', nick: '', \\n    isLoggedIn: false, isSignedUp: false, \\n    age: \\\"\\\", gender: \\\"\\\"\\n  }\\n]\\n\\nexport const signUpNewAccount =  createAsyncThunk(\\n    'user/signUpNewAccount', \\n    async data => {\\n        const requestOptions = {\\n            method: 'POST',\\n            headers: { 'Content-Type': 'application/json' },\\n            body: JSON.stringify(data),\\n        }\\n        const response = await fetch('http://localhost:5000/signUp', requestOptions)\\n            .then(res => res.json())\\n        return response\\n\\n    }\\n)\\n\\nexport const loadProfile =  createAsyncThunk(\\n  'user/loadProfile', \\n  async username => {\\n      const response = await fetch('http://localhost:5000/profiles/' + String(username))\\n          .then(res => res.json())\\n      return response\\n\\n  }\\n)\\n\\nexport const loadOthersProfile =  createAsyncThunk(\\n  'user/loadOthersProfile', \\n  async username => {\\n      const response = await fetch('http://localhost:5000/profiles/' + String(username))\\n          .then(res => res.json())\\n      return response\\n\\n  }\\n)\\n\\nexport const editProfile =  createAsyncThunk(\\n  'user/editProfile', \\n  async data => {\\n      const requestOptions = {\\n          method: 'POST',\\n          headers: { 'Content-Type': 'application/json' },\\n          body: JSON.stringify(data),\\n      }\\n      const response = await fetch('http://localhost:5000/editProfile', requestOptions)\\n          .then(res => res.json())\\n\\n      return response\\n\\n  }\\n)\\n\\n\\nconst userSlice = createSlice({\\n  name: 'user',\\n  initialState,\\n  reducers: {\\n    LogIn(state, action) {\\n        const { id, pwd, nick } = action.payload\\n        state[0].id = id\\n        state[0].pwd = pwd\\n        state[0].nick = nick\\n        state[0].isLoggedIn = true\\n    },\\n    LogOut(state, action) {\\n        state[0].id = ''\\n        state[0].pwd = ''\\n        state[0].nick = ''\\n        state[0].isLoggedIn = false\\n    }\\n  },\\n  extraReducers(builder) {\\n    builder\\n      .addCase(signUpNewAccount.fulfilled, (state, action) => {\\n        state[0].isSignedUp = true\\n      })\\n      .addCase(loadProfile.fulfilled, (state, action) => {\\n        if (action.payload.age !== undefined) {\\n          state[0].age = action.payload.age\\n        }\\n        if (action.payload.nickname !== undefined) {\\n          state[0].nick = action.payload.nickname\\n        }\\n        if (action.payload.gender !== undefined) {\\n          state[0].gender = action.payload.gender\\n        }\\n      })\\n      .addCase(editProfile.fulfilled, (state, action) => {\\n        if (action.payload.age !== undefined) {\\n          state[0].age = action.payload.age\\n        }\\n        if (action.payload.nickname !== undefined) {\\n          state[0].nick = action.payload.nickname\\n        }\\n        if (action.payload.gender !== undefined) {\\n          state[0].gender = action.payload.gender\\n        }\\n      })\\n  }\\n})\\n\\nexport default userSlice.reducer\\n\\nexport const { LogIn, LogOut } = userSlice.actions\\n\\nexport const getUserID= state => state.user[0].id\\n\\nexport const getUserState = state => state.user[0].isLoggedIn\\n\\nexport const getUserNick = state => state.user[0].nick\\n\\nexport const getUserAge = state => state.user[0].age\\n\\nexport const getUserGender = state => state.user[0].gender\\n\\n\\n\\n\\n\\n\",\"import React from 'react'\\nimport { Link } from 'react-router-dom'\\nimport { useDispatch } from 'react-redux'\\nimport { LogOut } from '../features/user/userSlice'\\nimport { useHistory } from 'react-router-dom';\\n\\nexport const Navbar = () => {\\n  const dispatch = useDispatch()\\n  const history = useHistory()\\n  const handleLogOut = () => {\\n    history.push('/home')\\n    dispatch(LogOut())\\n  }\\n\\